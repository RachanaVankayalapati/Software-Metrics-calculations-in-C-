#include<stdio.h>
 int main()
{
int n,r,sum=0,temp;
char s,d,f,s;
double ha,fh,ms;
printf("enter the number=");
scanf("%d",&n);
temp=n;
//rwst
//rweyh
while(n>0)
{
/*sfiksf*/
/*fwuseht*/
r=n%10;
sum=sum+(r*r*r);
n=n/10;
}
if(temp==sum)
printf("armstrong number ");
else
printf("not armstrong number");
return 0;
}
